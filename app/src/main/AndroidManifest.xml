<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stocksimple">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".baseStock"></activity>
        <activity android:name=".basePrecios" />
        <activity android:name=".baseDatos" />
        <activity android:name=".precios_alta" />
        <activity android:name=".stock_listado" />
        <activity android:name=".stock_consulta" />
        <activity android:name=".precios_principal" />
        <activity android:name=".stock_principal" />
        <activity android:name=".precios_listado" />
        <activity android:name=".precios_consulta" />
        <activity android:name=".stock_ventas" />
        <activity android:name=".stock_compras" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>